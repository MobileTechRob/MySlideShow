<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MySlideShow.UnitTestPage"
             Title="UnitTestPage">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button 
            x:Name="SpecificFromAndToDate"
            Text="Specific From and To Date"            
            Clicked="SpecificFromAndToDate_Clicked"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />


        <Button 
            x:Name="MinutesTest"
            Text="Minutes Test"            
            Clicked="MinutesTest_Clicked"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button 
            x:Name="HoursTest"
            Text="Hours Test"            
            Clicked="HoursTest_Clicked"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button 
            x:Name="DaysTest"
            Text="Days Test"            
            Clicked="DaysTest_Clicked"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button 
            x:Name="WeeksTest"
            Text="Weeks Test"            
            Clicked="WeeksTest_Clicked"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

    </VerticalStackLayout>
</ContentPage>